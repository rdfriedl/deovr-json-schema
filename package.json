{
  "name": "deovr-json-schema",
  "version": "0.1.2",
  "types": "dist/index.d.ts",
  "author": "Robert Friedl <rdfriedl@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "ts-json-schema-generator --path src/index.ts --type DeoVrJson -o dist/schema.json --additional-properties true",
    "postbuild": "tsc",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "ts-json-schema-generator": "^1.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  }
}
